{
  "name": "lowlow",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "postcss-custom-properties": "^5.0.2",
    "react-scripts": "^0.9.0",
    "serve": "^3.2.10"
  },
  "dependencies": {
    "clipboard": "^1.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "showdown": "^1.6.4"
  },
  "now": {
    "name": "lowlow",
    "alias": "lowlow"
  },
  "scripts": {
    "now-start": "serve build/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "now && now alias"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "postcss": {
    "map": false,
    "plugins": {
      "postcss-custom-properties": {
        "preserve": true
      }
    }
  }
