{
  "name": "lowlow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "next": "^9.4.3",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "showdown": "^1.9.0",
    "tailwindcss": "^1.4.6"
  },
  "scripts": {
    "dev": "next dev -p 3567",
    "build": "next build",
    "start": "next start",
    "types": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "prettier": {
    "printWidth": 85,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --parser postcss --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.17",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.4",
    "@types/showdown": "^1.9.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  }
}
