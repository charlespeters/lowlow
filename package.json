{
  "name": "lowlow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.2",
    "showdown": "^1.9.0"
  },
  "scripts": {
    "dev": "PORT=3567 react-scripts start",
    "now-build": "react-scripts build && mv build dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "types": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "prettier": {
    "printWidth": 85,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.0.11",
    "@types/showdown": "^1.9.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  }
}
